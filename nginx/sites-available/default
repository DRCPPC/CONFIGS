server {
        listen 80 default_server;
        listen [::]:80 default_server;

        root /home/ubuntu/static/default;
        index index.html index.htm index.nginx-debian.html;

        server_name _;

        location / {
                try_files $uri $uri/ /index.html;
                return 301 https://ppc-drc.com;
        }
}